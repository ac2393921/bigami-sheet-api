# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type School {
  id: ID!
  name: String!
  style: String!
  enemy: String!
}

type LowerSchool {
  id: ID!
  school_id: ID!
  name: String!
  style: String!
  enemy: String!
}

type Query {
  schools: [School!]!
  lower_schools: [LowerSchool!]!
}
